---
import Layout from "../layouts/Layout.astro";
import { htmlOutput } from "../hello.js";
const file = "./hello.js";
const matches = import.meta.glob("./*.js", {
  eager: true,
});

const html = matches[file]?.htmlOutput.html;
---

<Layout>
  <Fragment set:html={html} />
  <Fragment set:html={htmlOutput.html} />
</Layout>
